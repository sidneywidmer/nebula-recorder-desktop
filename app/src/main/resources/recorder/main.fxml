<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Text?>

<AnchorPane fx:id="main" prefHeight="300.0" prefWidth="600.0" xmlns:fx="http://javafx.com/fxml/1" fx:controller="recorder.controllers.MainController">
    <children>
      <Text layoutX="36.0" layoutY="42.0" strokeType="OUTSIDE" strokeWidth="0.0" text="Yay, logged in!" />
      <Button fx:id="select" onAction="#selectArea" layoutX="36.0" layoutY="71.0" mnemonicParsing="false" prefHeight="23.0" prefWidth="109.0" text="Select Area" />
      <Button fx:id="start" onAction="#startRecording" layoutX="36.0" layoutY="101.0" mnemonicParsing="false" prefHeight="23.0" prefWidth="109.0" text="Start Recording" />
      <Button fx:id="stop" onAction="#stopRecording" layoutX="36.0" layoutY="131.0" mnemonicParsing="false" prefHeight="23.0" prefWidth="109.0" text="End Recording" />
    </children>
</AnchorPane>
