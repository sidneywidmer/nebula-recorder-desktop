<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Text?>

<AnchorPane fx:id="login" prefHeight="300.0" prefWidth="600.0" xmlns:fx="http://javafx.com/fxml/1" fx:controller="recorder.controllers.LoginController">
    <children>
        <VBox layoutX="24.0" layoutY="31.0" prefHeight="169.0" prefWidth="185.0" spacing="5.0">
            <children>
                <Text fx:id="error" fill="#dd0404" strokeType="OUTSIDE" strokeWidth="0.0" text="Text" visible="false">
                    <VBox.margin>
                        <Insets bottom="20.0" />
                    </VBox.margin>
                </Text>
                <Text strokeType="OUTSIDE" strokeWidth="0.0" text="E-Mail" />
                <TextField fx:id="email" />
                <Text strokeType="OUTSIDE" strokeWidth="0.0" text="Password" />
                <TextField fx:id="password" />
                <Button mnemonicParsing="false" onAction="#login" text="Login">
                    <VBox.margin>
                        <Insets top="10.0" />
                    </VBox.margin>
                </Button>
            </children>
        </VBox>
    </children>
</AnchorPane>
